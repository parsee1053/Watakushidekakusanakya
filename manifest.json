{
  "manifest_version": 2,
  "name": "Watakushidekakusanakya",
  "version": "0.3",
  "description": "Images are hidden by Mito Tsukino.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "mito.png"
  ]
}